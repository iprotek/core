{
    "name": "iprotek/core",
    "description": "A simple design for UI and core functions of iprotek",
    "keywords": ["laravel", "plugin", "iprotek-core"],
    "authors": [
        {
            "name": "Joseph Aguilar",
            "email": "admin@iprotek.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "iProtek\\Core\\": "src/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php artisan vendor:publish --tag=public --force"
        ]
    },
    "require": {
        "dompdf/dompdf": "^2.0",
        "php": ">=7.0.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "iprotek/data":"dev-main",
        "iprotek/sys-notification":"dev-main",
        "iprotek/sms-sender":"dev-main",
        "iprotek/xrac":"dev-main",
        "iprotek/device":"dev-main",
        "iprotek/apps":"dev-main",
        "iprotek/account":"dev-main",
        "iprotek/dbm":"dev-main",
        "pusher/pusher-php-server": "^7.2",
        "phpoffice/phpspreadsheet": "^3.8",
        "simplesoftwareio/simple-qrcode": "dev-main",
        "milon/barcode": "dev-main"
    },
    "type": "library",  
    "license": "MIT", 
    "require-dev": {
    },
    "suggest": {
        "ext-intl": "Use this for design your dashboard on bootstrap."
    }, 
    "extra": {
        "laravel": {
            "providers": [
                "iProtek\\Core\\iProtekServiceProvider"
            ]
        }
    },
    "version": "1.8.0" 
}
